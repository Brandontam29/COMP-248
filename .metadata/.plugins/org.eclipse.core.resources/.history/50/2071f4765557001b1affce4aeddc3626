package animals;

public class Animal {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

	public static String introduce() {
		return "I am a dog";
	}

	public static void entertain() {
		int a = 5;
		int b = 10;

		for (int i = 0; i < a; i += 1) {
			b+1;
			System.out.println(b);
			return;
		}
		
		System.out.println("finished!");
	}

}
